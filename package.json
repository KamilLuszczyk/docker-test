{
  "name": "docker-test",
  "version": "0.1.0",
  "description": "Sample docker image to test various scenarios with Docker.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "d-build": "docker build -t stefanwalther/hello-world .",
    "d-run": "docker run -it stefanwalther/hello-world",
    "start": "node ./index.js",
    "docs": "docker run --rm -v ${PWD}:/opt/verb stefanwalther/verb",
    "dc-up:devb": "docker-compose --f=./docker-compose.dev.yml up --build",
    "dc-up:dev": "docker-compose --f=./docker-compose.dev.yml up",
    "precommit": "npm run docs && ./commit-if-changed.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanwalther/docker-nodejs-hello-world.git"
  },
  "keywords": [
    "docker",
    "hello-world",
    "node.js"
  ],
  "author": "Stefan Walther <swr.nixda@gmail.com> (http://qliksite.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanwalther/docker-nodejs-hello-world/issues"
  },
  "homepage": "https://github.com/stefanwalther/docker-nodejs-hello-world#readme",
  "dependencies": {
    "express": "^4.15.3",
    "winster": "^0.2.5"
  },
  "devDependencies": {
    "husky": "^0.14.3"
  }
}
